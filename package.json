{
  "name": "@beforeyoubid/graphql-schema-diff",
  "version": "0.0.3",
  "description": "Diff GraphQL schemas to find differences between schema versions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/beforeyoubid/graphql-schema-diff/issues"
  },
  "homepage": "https://github.com/beforeyoubid/graphql-schema-diff#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:beforeyoubid/graphql-schema-diff.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint '**/*.{ts,tsx,js,jsx}' --quiet",
    "ts": "tsc --noEmit",
    "build-and-publish": "npm run build && npm publish --access public",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src/*.ts --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "graphql": "^15.5.0"
  },
  "devDependencies": {
    "@babel/cli": "7.18.6",
    "@babel/core": "7.18.6",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@swc/core": "^1.2.212",
    "@swc/jest": "^0.2.21",
    "@types/jest": "28.1.5",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "asl-types": "^1.2.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.2",
    "jest-junit": "14.0.0",
    "pre-commit": "^1.1.3",
    "prettier": "2.7.1",
    "ts-node": "^10.8.2",
    "typescript": "4.7.4"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [],
  "license": "MIT",
  "files": [
    "dist/**/*"
  ]
}
